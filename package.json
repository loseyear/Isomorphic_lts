{
  "name": "Isomorphic_lts",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "start": "node bin/entry.js",
    "start:supervisor": "supervisor bin/entry.js",
    "start:pm2": "pm2 start bin/entry.js --name=mobile",
    "stop:pm2": "pm2 stop mobile",
    "reload:pm2": "pm2 reload mobile",
    "build": "webpack --config build/webpack.config.js",
    "build:watch": "cd build && webpack -w",
    "build:react": "cd build && webpack --config react.config.js",
    "wds": "cd build && webpack-dev-server --devtool eval --progress --colors --port 9527 --inline --hot --config wds.config.js --content-base ./../",
    "mock": "karma start ",
    "mocha": "mocha --compilers js:babel-core/register mock/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loseyear/Isomorphic_lts.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/loseyear/Isomorphic_lts/issues"
  },
  "homepage": "https://github.com/loseyear/Isomorphic_lts#readme",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.24.1",
    "css-modules-require-hook": "^4.0.6",
    "extract-text-webpack-plugin": "^2.1.0",
    "kcors": "^1.3.2",
    "koa": "next",
    "koa-convert": "^1.2.0",
    "koa-onerror": "^3.1.0",
    "koa-static": "^3.0.0",
    "mini-logger": "^1.1.2",
    "node-sass": "^4.5.2",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.0",
    "react-router-dom": "^4.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.3.0",
    "webpack": "^2.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.11.1",
